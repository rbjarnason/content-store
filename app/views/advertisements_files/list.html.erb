<h1>Listing advertisements_files</h1>

<table>
  <tr>
  <% for column in AdvertisementsFile.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  </tr>
  
<% for advertisements_file in @advertisements_files %>
  <tr>
  <% for column in AdvertisementsFile.content_columns %>
    <td><%=h advertisements_file.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Show', :action => 'show', :id => advertisements_file %></td>
    <td><%= link_to 'Edit', :action => 'edit', :id => advertisements_file %></td>
    <td><%= link_to 'Destroy', { :action => 'destroy', :id => advertisements_file }, :confirm => 'Are you sure?', :method => :post %></td>
  </tr>
<% end %>
</table>

<%= link_to 'Previous page', { :page => @advertisements_file_pages.current.previous } if @advertisements_file_pages.current.previous %>
<%= link_to 'Next page', { :page => @advertisements_file_pages.current.next } if @advertisements_file_pages.current.next %> 

<br />

<%= link_to 'New advertisements_file', :action => 'new' %>
